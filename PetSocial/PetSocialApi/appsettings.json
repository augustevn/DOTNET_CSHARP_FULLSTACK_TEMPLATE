{
  "ConnectionStrings": {
    "DefaultConnection": "<insert:production-connectionstring>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SmtpConfig": {
    "Host": "<insert:smtp-host>",
    "Port": 465,
    "UseSsl": true,
    "Email": "<insert:mailbox-email-address>",
    "Password": "<insert:mailbox-password>"
  },
  "JwtConfig": {
    "Secret": "<random-string-as-jwt-secret-prod-here>",
    "TTL": "168:00:00"
  }
}
